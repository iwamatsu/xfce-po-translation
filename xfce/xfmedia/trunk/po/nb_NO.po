#: ../src/main.c:228
#, c-format
msgid ""
"Usage: %s [options] [files]\n"
"\n"
msgstr ""
"Bruk: %s [opsjoner] [filer]\n"

#: ../src/main.c:229 ../src/xfmedia-remote.c:104
msgid "    -h, --help           Print this help message and exit.\n"
msgstr "    -h, --help           Skriv ut denne meldingen og avslutt.\n"

#: ../src/main.c:230 ../src/xfmedia-remote.c:105
msgid "    -V, --version        Print version information and exit.\n"
msgstr ""
"    -V, --version        Skriv ut versjonsinformasjon og avslutt.\n"

#: ../src/main.c:231
msgid ""
"    -v, --verbose        Instruct xine to print useful status information.\n"
msgstr ""
"    -v, --verbose        Instruer xine å skrive nyttig statusinformasjon.\n"

#: ../src/main.c:232
msgid ""
"    --debug              Instruct xine to print detailed debug information.\n"
msgstr ""
"    --debug              Instruer xine å skrive ut detaljert debug\n"
"                          informasjon.\n"

#: ../src/main.c:233
msgid ""
"    -f, --fullscreen     Start xfmedia's video window in fullscreen mode.\n"
msgstr ""
"    -f, --fullscreen     Start xfmedia's video vindu i fullskjermmodus.\n"

#: ../src/main.c:234
msgid ""
"    -p, --autoplay       Start playing when Xfmedia starts.  (This is the\n"
"                         default if xfmedia is given filenames on the "
"command\n"
"                         line.)\n"
msgstr ""
"    -p, --autoplay       Start avspilling når Xfmedia starter. (Dette er\n"
"                         standard hvis xfmedia får angitt filnavn via "
"                         kommandolinjen.\n"

#: ../src/main.c:237
msgid "    -c, --clear-playlist Start xfmedia with an empty playlist.\n"
msgstr "    -c, --clear-playlist Start xfmedia med tom spilleliste.\n"

#: ../src/main.c:238
msgid "    --audio-out=DRIVER   Specify the audio output driver to use.\n"
msgstr "    --audio-out=DRIVER   Spesifiser lyddriver som skal brukes.\n"

#: ../src/main.c:239
msgid "    --video-out=DRIVER   Specify the video output driver to use.\n"
msgstr "    --video-out=DRIVER   Spesifiser video driver som skal brukes.\n"

#: ../src/main.c:240
msgid ""
"    --vwin-geometry=WxH[+X+Y]\n"
"                         Specify a fixed size and/or position for xfmedia's\n"
"                         video window.\n"
msgstr ""
"    --vwin-geometry=WxH[+X+Y]\n"
"                         Spesifiser en fast størrelse og/eller posisjon for\n"
"                         Xfmedia sitt video vindu.\n"

