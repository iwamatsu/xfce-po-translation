#: ../src/main.c:228
#, c-format
msgid ""
"Usage: %s [options] [files]\n"
"\n"
msgstr ""
"Sử dụng: %s [các tùy chọn] [các tập tin]\n"
"\n"

#: ../src/main.c:229 ../src/xfmedia-remote.c:104
msgid "    -h, --help           Print this help message and exit.\n"
msgstr "    -h, --help           In trợ giúp này và thoát.\n"

#: ../src/main.c:230 ../src/xfmedia-remote.c:105
msgid "    -V, --version        Print version information and exit.\n"
msgstr "    -V, --version        In thông tin phiên bản và thoát.\n"

#: ../src/main.c:231
msgid ""
"    -v, --verbose        Instruct xine to print useful status information.\n"
msgstr ""
"    -v, --verbose        Chỉ thị cho xine in thông tin trạng thái có ích.\n"

#: ../src/main.c:232
msgid ""
"    --debug              Instruct xine to print detailed debug information.\n"
msgstr ""
"    --debug              Chỉ thị cho xine in thông tin tìm sửa lỗi chi "
"tiết.\n"

#: ../src/main.c:233
msgid ""
"    -f, --fullscreen     Start xfmedia's video window in fullscreen mode.\n"
msgstr ""
"    -f, --fullscreen     Chạy cửa sổ hình video của xfmedia ở dạng toàn màn "
"hình.\n"

#: ../src/main.c:234
msgid ""
"    -p, --autoplay       Start playing when Xfmedia starts.  (This is the\n"
"                         default if xfmedia is given filenames on the "
"command\n"
"                         line.)\n"
msgstr ""
"    -p, --autoplay       Bắt đầu nghe nhạc khi chạy Xfmedia.  (Đây là mặc\n"
"                         định nếu có đưa tên tập tin vào dòng lệnh.)\n"

#: ../src/main.c:237
msgid "    -c, --clear-playlist Start xfmedia with an empty playlist.\n"
msgstr "    -c, --clear-playlist Chạy xfmedia với một danh sách rỗng.\n"

#: ../src/main.c:238
msgid "    --audio-out=DRIVER   Specify the audio output driver to use.\n"
msgstr "    --audio-out=DRIVER   Chỉ ra driver âm thanh đầu ra để sử dụng.\n"

#: ../src/main.c:239
msgid "    --video-out=DRIVER   Specify the video output driver to use.\n"
msgstr "    --video-out=DRIVER   Chỉ ra driver hình ảnh đầu ra để sử dụng.\n"

#: ../src/main.c:240
msgid ""
"    --vwin-geometry=WxH[+X+Y]\n"
"                         Specify a fixed size and/or position for xfmedia's\n"
"                         video window.\n"
msgstr ""
"    --vwin-geometry=WxH[+X+Y]\n"
"                         Chỉ ra kích thước cố định và/hoặc vị trí cho cửa\n"
"                         sổ hình ảnh của xfmedia.\n"

#: ../src/main.c:315 ../src/xfmedia-remote.c:167
