#: ../src/main.c:227 ../src/xfmedia-remote.c:102
#, c-format
msgid ""
"Xfmedia %s.  Released under the terms of the GNU GPL.  See the COPYING\n"
"file for details.\n"
"\n"
msgstr ""
"ཨེགསི་ཨེཕ་བརྡ་བརྒྱུད་ %s.  ཇི་ཨེན་ཡུ་ ཇི་པི་ཨེལ་གྱི་དོན་ཚིག་འོག་ལུ་  གསར་བཏོན་འབད་ནུག།  རྒྱས་བཤད་ཀྱི་"
"དོན་ལུ་  འདྲ་བཤུས་བརྐྱབ་དོ་\n"
"བལྟ་ད།\n"
"\n"

#: ../src/main.c:228
#, c-format
msgid ""
"Usage: %s [options] [files]\n"
"\n"
msgstr ""
"བེད་སྤྱོད: %s [options] [files]\n"
"\n"

#: ../src/main.c:229 ../src/xfmedia-remote.c:104
msgid "    -h, --help           Print this help message and exit.\n"
msgstr "    -h, --help           གྲོགས་རམ་འཕྲིན་དོན་འདི་དཔར་བསྐྲུན་འབད་ཞིནམ་ལས་ཕྱིར་ཐོན།\n"

#: ../src/main.c:230 ../src/xfmedia-remote.c:105
msgid "    -V, --version        Print version information and exit.\n"
msgstr "    -V, --version        འཐོན་རིམ་བརྡ་དོན་དཔར་བསྐྲུན་འབད་ཞིནམ་ལས་ཕྱིར་ཐོན།\n"

#: ../src/main.c:231
msgid ""
"    -v, --verbose        Instruct xine to print useful status information.\n"
msgstr ""
"    -v, --verbose        ཁག་ཆེ་བའི་གནས་ཚད་བརྡ་དོན་དཔར་བསྐྲུན་འབད་ནི་ལུ་  ཨེགསི་ཨའི་ཨེན་ཨི་"
"བསླབ་སྟོན་བྱིན།I\n"

#: ../src/main.c:232
msgid ""
"    --debug              Instruct xine to print detailed debug information.\n"
msgstr ""
"    --debug             རྐྱེན་སེལ་བརྡ་དོན་རྒྱས་བཤད་དཔར་བསྐྲུན་འབད་ནི་ལུ་  ཨེགསི་ཨའི་ཨེན་ཨི་བསླབ་"
"སྟོན་བྱིན། \n"

#: ../src/main.c:233
msgid ""
"    -f, --fullscreen     Start xfmedia's video window in fullscreen mode.\n"
msgstr ""
"    -f, --fullscreen     གསལ་གཞི་གངམ་ཐབས་ལམ་ནང་ལུ་  ཨེགསི་ཨེཕ་བརྡ་བརྒྱུད་ཀྱི་ཝི་ཌིའོ་སྒོ་སྒྲིག་"
"འགོ་བཙུགས།\n"

#: ../src/main.c:234
msgid ""
"    -p, --autoplay       Start playing when Xfmedia starts.  (This is the\n"
"                         default if xfmedia is given filenames on the "
"command\n"
"                         line.)\n"
msgstr ""
"    -p, --autoplay       ཨེགསི་ཨེཕ་བརྡ་བརྒྱུད་འགོ་བཙུགཔ་ད་  གཏང་ནི་འདི་འགོ་བཙུགས།   (བརྡ་"
"བཀོད་གྱལ་རིམ་གུ་\n"
"                         ཨེགསི་ཨེཕ་བརྡ་བརྒྱུད་འདི་ཡིག་སྣོད་མིང་གུ་བྱིན་ཡོད་པ་ཅིན་\n"
"                         སྔོན་སྒྲིག་ཨིན།)\n"

#: ../src/main.c:237
msgid "    -c, --clear-playlist Start xfmedia with an empty playlist.\n"
msgstr "    -c, --clear-གཏང་ཐོ་འགོ་བཙུགས་ཨེགསི་ཨེཕ་བརྡ་བརྒྱུད་འདི་  གཏང་ཐོ་སྟོངམ་དང་ཅི་ཁར།\n"

#: ../src/main.c:238
msgid "    --audio-out=DRIVER   Specify the audio output driver to use.\n"
msgstr ""
"    --audio-out=DRIVER   ལག་ལེན་འཐབ་ནི་དོན་ལུ་  རྣར་ཉན་ཨའུཊི་པུཊི་འདྲེན་བྱེད་འདི་གསལ་བཀོད་"
"འབད།\n"

#: ../src/main.c:239
msgid "    --video-out=DRIVER   Specify the video output driver to use.\n"
msgstr ""
"    --video-out=DRIVER   ལག་ལེན་འཐབ་ནི་ལུ་  ཝི་ཌིའོ་ཨའུཊི་པུཊི་འདྲེན་བྱེད་འདི་གསལ་བཀོད་འབད།\n"

#: ../src/main.c:240
msgid ""
"    --vwin-geometry=WxH[+X+Y]\n"
"                         Specify a fixed size and/or position for xfmedia's\n"
"                         video window.\n"
msgstr ""
"    --vwin-geometry=WxH[+X+Y]\n"
"                         ཨེགསི་ཨེཕ་བརྡ་བརྒྱུད་ཀྱི་ཝི་ཌིའོ་སྒོ་སྒྲིག་དོན་ལུ་  ཚུད་སྒྲིག་འབད་དེ་ཡོད་པའི་"
"ཚད་དང་/ ཡང་ན་  གནས་ས་འདི་\n"
"                         གསལ་བཀོད་འབད།\n"

