#: ../src/main.c:228
#, c-format
msgid ""
"Usage: %s [options] [files]\n"
"\n"
msgstr ""
"Käyttö: %s [valitsimet] [tiedostot]\n"
"\n"

#: ../src/main.c:229 ../src/xfmedia-remote.c:104
msgid "    -h, --help           Print this help message and exit.\n"
msgstr "    -h, --help           Tulosta tämä ohje ja poistu.\n"

#: ../src/main.c:230 ../src/xfmedia-remote.c:105
msgid "    -V, --version        Print version information and exit.\n"
msgstr "    -V, --version        Tulosta versiotiedot ja poistu.\n"

#: ../src/main.c:231
msgid ""
"    -v, --verbose        Instruct xine to print useful status information.\n"
msgstr ""
"    -v, --verbose        Xine tulostaa käytännöllisiä status-tietoja.\n"

#: ../src/main.c:232
msgid ""
"    --debug              Instruct xine to print detailed debug information.\n"
msgstr ""
"    --debug              Xine tulostaa yksityiskohtaisia "
"vianjäljitystietoja.\n"

#: ../src/main.c:233
msgid ""
"    -f, --fullscreen     Start xfmedia's video window in fullscreen mode.\n"
msgstr "    -f, --fullscreen     Avaa Xfmedian videoikkuna täysruututilassa.\n"

#: ../src/main.c:234
msgid ""
"    -p, --autoplay       Start playing when Xfmedia starts.  (This is the\n"
"                         default if xfmedia is given filenames on the "
"command\n"
"                         line.)\n"
msgstr ""
"    -p, --autoplay       Aloita soitto Xfmedian käynnistyessä.  (Tämä on\n"
"                         oletus, jos Xfmedialle määritetään tiedostonimiä\n"
"                         komentorivillä.)\n"

#: ../src/main.c:237
msgid "    -c, --clear-playlist Start xfmedia with an empty playlist.\n"
msgstr "    -c, --clear-playlist Käynnistää xfmedian soittolista tyhjänä.\n"

#: ../src/main.c:238
msgid "    --audio-out=DRIVER   Specify the audio output driver to use.\n"
msgstr "    --audio-out=AJURI    Määritä käytettävä ääniajuri.\n"

#: ../src/main.c:239
msgid "    --video-out=DRIVER   Specify the video output driver to use.\n"
msgstr "    --video-out=AJURI    Määritä käytettävä videoajuri.\n"

#: ../src/main.c:240
msgid ""
"    --vwin-geometry=WxH[+X+Y]\n"
"                         Specify a fixed size and/or position for xfmedia's\n"
"                         video window.\n"
msgstr ""
"    --vwin-geometry=LxK[+X+Y]\n"
"                         Määritä xfmedian videoikkunalle kiinteä koko ja/"
"tai\n"
"                         sijainti.\n"

