#: ../src/main.c:228
#, c-format
msgid ""
"Usage: %s [options] [files]\n"
"\n"
msgstr ""
"Użycie: %s [opcje] [pliki]\n"
"\n"

#: ../src/main.c:229 ../src/xfmedia-remote.c:104
msgid "    -h, --help           Print this help message and exit.\n"
msgstr "  -h, --help                wypisuje tą treść pomocy.\n"

#: ../src/main.c:230 ../src/xfmedia-remote.c:105
msgid "    -V, --version        Print version information and exit.\n"
msgstr "  -V, --version             wypisuje wersję i kończy działanie.\n"

#: ../src/main.c:231
msgid ""
"    -v, --verbose        Instruct xine to print useful status information.\n"
msgstr "  -V, --version             wypisuje informacje wersji\n"

#: ../src/main.c:232
msgid ""
"    --debug              Instruct xine to print detailed debug information.\n"
msgstr "    --debug              Wypisuje szczegółowe informacje debugera.\n"

#: ../src/main.c:233
msgid ""
"    -f, --fullscreen     Start xfmedia's video window in fullscreen mode.\n"
msgstr ""
"    -f, --fullscreen     Uruchom okno wyjścia wideo w trybie "
"pełnoekranowym.\n"

#: ../src/main.c:234
msgid ""
"    -p, --autoplay       Start playing when Xfmedia starts.  (This is the\n"
"                         default if xfmedia is given filenames on the "
"command\n"
"                         line.)\n"
msgstr ""
"    -p, --autoplay       Rozpoczyna odtwarzanie przy starcie xfmedia.\n"
"                         (Domyślnie jeśli xfmedia uruchamiane jest z "
"plikami\n"
"                         w lini poleceń)\n"

#: ../src/main.c:237
msgid "    -c, --clear-playlist Start xfmedia with an empty playlist.\n"
msgstr ""
"    -c, --clear-playlist Uruchamia xfmedia z pustą listą odtwarzania.\n"

#: ../src/main.c:238
msgid "    --audio-out=DRIVER   Specify the audio output driver to use.\n"
msgstr "    --audio-out=STEROWNIK   Określa sterownik wyjściowy dla dźwięku.\n"

#: ../src/main.c:239
msgid "    --video-out=DRIVER   Specify the video output driver to use.\n"
msgstr "    --video-out=STEROWNIK   Określa sterownik wyjściowy dla obrazu.\n"

#: ../src/main.c:240
msgid ""
"    --vwin-geometry=WxH[+X+Y]\n"
"                         Specify a fixed size and/or position for xfmedia's\n"
"                         video window.\n"
msgstr ""
"    --vwin-geometry=WxH[+X+Y]\n"
"                         Podaj określone i/lub rozmiar i pozycję dla okna\n"
"                         wideo xfmedia.\n"

