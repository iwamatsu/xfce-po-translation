#: ../src/xfmedia-remote.c:103
#, c-format
msgid ""
"Usage: %s [options]\n"
"\n"
msgstr ""
"Penggunaan: %s [opsi]\n"
"\n"

#: ../src/xfmedia-remote.c:106
msgid ""
"    --session=N          Specify the number of the already-running instance "
"of\n"
"                         Xfmedia.  (Defaults to 0, or the first instance "
"found.)\n"
msgstr ""
"    --session=N          Tentukan nomor instansi yang telah berjalan"
"dari\n"
"                         Xfmedia.  (Standar adalah 0, atau instansi pertama yang "
"ditemukan.)\n"

#: ../src/xfmedia-remote.c:108
msgid ""
"    --ping               Checks to see if an instance of Xfmedia is "
"running.\n"
msgstr ""
"    --ping               Cek untuk melihat jika ada instansi Xfmedia yang"
"berjalan.\n"

#: ../src/xfmedia-remote.c:109
msgid ""
"    -p, --play[=N]       Causes Xfmedia to play the file at playlist idx N.\n"
"                         If N is not specified, Xfmedia will play the "
"currently-\n"
"                         selected entry, or, if no item is selected, the "
"first\n"
"                         entry.  If used in conjunction with --playlist-"
"add,\n"
"                         will cause Xfmedia to play the newly-added entry.\n"
msgstr ""
"    -p, --play[=N]       Menyebabkan Xfmedia untuk memutar indeks berkas N di senarai putar.\n"
"                         Jika N tak ditentukan, Xfmedia akan memutar "
"entri\n"
"                         yang saat ini terpilih, atau, jika tak ada item terpilih, "
"entri\n"
"                         pertama.  Jika digunakan secara bersamaan dengan --playlist-"
"add,\n"
"                         akan menyebabkan Xfmedia memutar entri yang baru ditambah.\n"

#: ../src/xfmedia-remote.c:114
msgid ""
"    --toggle             Causes Xfmedia to pause if playing, or play if "
"paused\n"
"                         or stopped.\n"
msgstr ""
"    --toggle             Menyebabkan Xfmedia untuk jeda jika sedang memutar, atau memutar jika"
"jeda\n"
"                         atau berhenti.\n"

#: ../src/xfmedia-remote.c:116
msgid "    --pause              Causes Xfmedia to pause/unpause.\n"
msgstr "    --pause              Menyebabkan Xfmedia untuk jeda/tak jeda.\n"

#: ../src/xfmedia-remote.c:117
msgid "    --stop               Causes Xfmedia to stop playback.\n"
msgstr "    --stop               Menyebabkan Xfmedia untuk berhenti putar ulang.\n"

#: ../src/xfmedia-remote.c:118
msgid ""
"    --previous           Causes Xfmedia to play the entry played before the\n"
"                         currently-selected (or -playing) entry.\n"
msgstr ""
"    --previous           Menyebabkan Xfmedia untuk memutar entri yang diputar\n"
"                         sebelum entri yang saat ini terpilih (atau putar).\n"

#: ../src/xfmedia-remote.c:120
msgid ""
"    --next               Causes Xfmedia to play the next entry in the "
"playlist.\n"
"                         If Xfmedia is in shuffle mode, a new entry will be\n"
"                         selected randomly.\n"
msgstr ""
"    --next               Menyebabkan Xfmedia untuk memutar entri berikutnya di"
"senarai putar.\n"
"                         Jika Xfmedia dalam mode acak, entri baru akan dipilih\n"
"                         secara acak.\n"

#: ../src/xfmedia-remote.c:123
msgid ""
"    -a, --playlist-add=FILE\n"
"                         Adds FILE to the end of the playlist.\n"
msgstr ""
"    -a, --playlist-add=BERKAS\n"
"                         Tambah BERKAS ke akhir senarai putar.\n"

#: ../src/xfmedia-remote.c:125
msgid ""
"    -r, --playlist-remove=N\n"
"                         Removes the specified playlist entry from the "
"playlist.\n"
msgstr ""
"    -r, --playlist-remove=N\n"
"                         Hapus entri senarai putar yang ditentukan dari"
"senarai putar.\n"

#: ../src/xfmedia-remote.c:127
msgid ""
"    -c, --playlist-clear\n"
"                         Removes all entries from the playlist.\n"
msgstr ""
"    -c, --playlist-clear\n"
"                         Hapus semua entri dari senarai putar.\n"

#: ../src/xfmedia-remote.c:129
msgid ""
"    -l, --playlist-load=FILE\n"
"                         Loads the specified playlist.  Note that this does "
"NOT\n"
"                         first clear the playlist.\n"
msgstr ""
"    -l, --playlist-load=BERKAS\n"
"                         Muat senarai putar yang ditentukan. Perlu dicatat bahwa ini "
"TIDAK\n"
"                         membersihkan senarai putar terlebih dahulu.\n"

#: ../src/xfmedia-remote.c:132
msgid ""
"    -s, --playlist-save=FILE\n"
"                         Saves the current playlist to the specified FILE.\n"
msgstr ""
"    -s, --playlist-save=BERKAS\n"
"                         Simpan senarai putar saat ini ke BERKAS yang ditentukan.\n"

#: ../src/xfmedia-remote.c:134
msgid "    -Q, --quit           Instructs Xfmedia to quit.\n"
msgstr "    -Q, --quit           Perintahkan Xfmedia untuk keluar.\n"

#: ../src/xfmedia-remote.c:135
msgid ""
"    -j, --jump-to-file   Causes Xfmedia to show the main window (if not "
"visible)\n"
"                         and show the jump-to-file entry box.\n"
msgstr ""
"    -j, --jump-to-file   Menyebabkan Xfmedia untuk menampilkan jendela utama (jika tak "
"tampak)\n"
"                         dan menampilkan kotak entri lompat ke berkas.\n"

#: ../src/xfmedia-remote.c:180
msgid "Argument to --session must be greater than or equal to 0.\n"
msgstr "Argumen untuk --session harus lebih besar atau sama dengan 0.\n"

#: ../src/xfmedia-remote.c:195
msgid "Argument to --play must be greater than or equal to 0.\n"
msgstr "Argumen untuk --play harus lebih besar atau sama dengan 0.\n"

#: ../src/xfmedia-remote.c:239
msgid "Argument to --playlist-remove must be greater than or equal to 0.\n"
msgstr "Argumen untuk --playlist-remove harus lebih besar atau sama dengan 0.\n"

