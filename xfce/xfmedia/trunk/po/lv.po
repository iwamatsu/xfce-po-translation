#: ../src/main.c:228
#, c-format
msgid ""
"Usage: %s [options] [files]\n"
"\n"
msgstr ""
"Izmantošana: %s [opcijas] [faili]\n"
"\n"

#: ../src/main.c:229
#: ../src/xfmedia-remote.c:104
msgid "    -h, --help           Print this help message and exit.\n"
msgstr "    -h, --help           Izdrukā šo palīdzības ziņu un iziet.\n"

#: ../src/main.c:230
#: ../src/xfmedia-remote.c:105
msgid "    -V, --version        Print version information and exit.\n"
msgstr "    -V, --version        Izdrukā versijas informāciju un iziet.\n"

#: ../src/main.c:231
msgid "    -v, --verbose        Instruct xine to print useful status information.\n"
msgstr "    -v, --verbose        Instruē xine drukāt noderīgu statusa informāciju.\n"

#: ../src/main.c:232
msgid "    --debug              Instruct xine to print detailed debug information.\n"
msgstr "    --debug              Instruē xine drukāt detalizētu atkļūdošanas informāciju.\n"

#: ../src/main.c:233
msgid "    -f, --fullscreen     Start xfmedia's video window in fullscreen mode.\n"
msgstr "    -f, --fullscreen     Palaist xfmedia video logu pilnekrāna režīmā.\n"

#: ../src/main.c:234
msgid ""
"    -p, --autoplay       Start playing when Xfmedia starts.  (This is the\n"
"                         default if xfmedia is given filenames on the command\n"
"                         line.)\n"
msgstr ""
"    -p, --autoplay       Sākt atskaņot, kad Xfmedia sāknējas. (Šī ir noklusētā\n"
"                         opcija, ja xfmedia ir dots faila nosaukums komandas\n"
"                         līnijā.)\n"

#: ../src/main.c:237
msgid "    -c, --clear-playlist Start xfmedia with an empty playlist.\n"
msgstr "    -c, --clear-playlist Sāknēt xfmedia ar tukšu atskaņošanas sarakstu.\n"

#: ../src/main.c:238
msgid "    --audio-out=DRIVER   Specify the audio output driver to use.\n"
msgstr "    --audio-out=DRIVER   Norāda izmantojamo izejošo audio draiveri.\n"

#: ../src/main.c:239
msgid "    --video-out=DRIVER   Specify the video output driver to use.\n"
msgstr "    --video-out=DRIVER   Norāda izmantojamo izejošo video draiveri.\n"

#: ../src/main.c:240
msgid ""
"    --vwin-geometry=WxH[+X+Y]\n"
"                         Specify a fixed size and/or position for xfmedia's\n"
"                         video window.\n"
msgstr ""
"    --vwin-geometry=WxH[+X+Y]\n"
"                         Norāda fiksētu xfmedia izmēru un/vai pozīciju\n"
"                         video logam.\n"

