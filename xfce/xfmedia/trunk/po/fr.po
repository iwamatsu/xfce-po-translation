#: ../src/xfmedia-remote.c:103
#, c-format
msgid ""
"Usage: %s [options]\n"
"\n"
msgstr ""
"Usage : %s [options]\n"
"\n"

#: ../src/xfmedia-remote.c:106
msgid ""
"    --session=N          Specify the number of the already-running instance of\n"
"                         Xfmedia.  (Defaults to 0, or the first instance found.)\n"
msgstr ""
"    --session=N          Spécifie le numéro d'une instance existante de\n"
"                         Xfmedia. (Par défaut à 0, ou la première instance trouvée.)\n"

#: ../src/xfmedia-remote.c:108
msgid "    --ping               Checks to see if an instance of Xfmedia is running.\n"
msgstr "    --ping               Vérifie si une instance de Xfmedia est en cours d'exécution.\n"

#: ../src/xfmedia-remote.c:109
msgid ""
"    -p, --play[=N]       Causes Xfmedia to play the file at playlist idx N.\n"
"                         If N is not specified, Xfmedia will play the currently-\n"
"                         selected entry, or, if no item is selected, the first\n"
"                         entry.  If used in conjunction with --playlist-add,\n"
"                         will cause Xfmedia to play the newly-added entry.\n"
msgstr ""
"    -p, --play[=N]       Demande à Xfmedia de jouer le fichier de la liste de\n"
"                         lecture à la position N. Si N est pas spécifié, Xfmedia\n"
"                         jouera l'entrée actuellement sélectionnée, ou, si aucune\n"
"                         entrée n'est sélectionnée, la première entrée.  Utilisé avec\n"
"                         --playlist-add demandera à Xfmedia de jouer l'entrée\n"
"                         nouvellement ajoutée.\n"

#: ../src/xfmedia-remote.c:114
msgid ""
"    --toggle             Causes Xfmedia to pause if playing, or play if paused\n"
"                         or stopped.\n"
msgstr ""
"    --toggle             Xfmedia se met en pause s'il est en lecture, ou se met en\n"
"                         lecture s'il est en pause ou arrêté.\n"

#: ../src/xfmedia-remote.c:116
msgid "    --pause              Causes Xfmedia to pause/unpause.\n"
msgstr "    --pause              Demande à Xfmedia de se mettre en/quitter la pause.\n"

#: ../src/xfmedia-remote.c:117
msgid "    --stop               Causes Xfmedia to stop playback.\n"
msgstr "    --stop               Demande à Xfmedia d'arrêter la lecture.\n"

#: ../src/xfmedia-remote.c:118
msgid ""
"    --previous           Causes Xfmedia to play the entry played before the\n"
"                         currently-selected (or -playing) entry.\n"
msgstr ""
"    --previous           Demande à Xfmedia de jouer l'entrée précédente à\n"
"                         l'entrée actuellement sélectionné (ou jouée).\n"

#: ../src/xfmedia-remote.c:120
msgid ""
"    --next               Causes Xfmedia to play the next entry in the playlist.\n"
"                         If Xfmedia is in shuffle mode, a new entry will be\n"
"                         selected randomly.\n"
msgstr ""
"    --next               Demande à Xfmedia de jouer l'entrée suivante dans la\n"
"                         liste de lecture. Si Xfmedia est en mode aléatoire, une\n"
"                         nouvelle entrée sera sélectionnée aléatoirement.\n"

#: ../src/xfmedia-remote.c:123
msgid ""
"    -a, --playlist-add=FILE\n"
"                         Adds FILE to the end of the playlist.\n"
msgstr ""
"    -a, --playlist-add=FICHIER\n"
"                         Ajoute le FICHIER à la fin de la liste de lecture.\n"

#: ../src/xfmedia-remote.c:125
msgid ""
"    -r, --playlist-remove=N\n"
"                         Removes the specified playlist entry from the playlist.\n"
msgstr ""
"    -r, --playlist-remove=N\n"
"                         Supprime l'entrée passée en paramètre de la liste de\n"
"                         lecture.\n"

#: ../src/xfmedia-remote.c:127
msgid ""
"    -c, --playlist-clear\n"
"                         Removes all entries from the playlist.\n"
msgstr ""
"    -c, --playlist-clear\n"
"                         Supprime toutes les entrées de la liste de lecture.\n"

#: ../src/xfmedia-remote.c:129
msgid ""
"    -l, --playlist-load=FILE\n"
"                         Loads the specified playlist.  Note that this does NOT\n"
"                         first clear the playlist.\n"
msgstr ""
"    -l, --playlist-load=FICHIER\n"
"                         Charge la liste de lecture passée en paramètre.  Notez\n"
"                          que cela NE vide PAS la liste de lecture avant.\n"

#: ../src/xfmedia-remote.c:132
msgid ""
"    -s, --playlist-save=FILE\n"
"                         Saves the current playlist to the specified FILE.\n"
msgstr ""
"    -s, --playlist-save=FICHIER\n"
"                         Sauve la liste de lecture courante dans le FICHIER passé en paramètre..\n"

#: ../src/xfmedia-remote.c:134
msgid "    -Q, --quit           Instructs Xfmedia to quit.\n"
msgstr "    -Q, --quit           indique à Xfmedia de quitter.\n"

#: ../src/xfmedia-remote.c:135
msgid ""
"    -j, --jump-to-file   Causes Xfmedia to show the main window (if not visible)\n"
"                         and show the jump-to-file entry box.\n"
msgstr ""
"    -j, --jump-to-file   Demande à Xfmedia de montrer la fenêtre principale (si elle n'est\n"
"                         pas visible) et de montrer la fenêtre de saut-au-fichier.\n"

#: ../src/xfmedia-remote.c:180
msgid "Argument to --session must be greater than or equal to 0.\n"
msgstr "L'argument de --session doit être supérieur ou égal à 0.\n"

#: ../src/xfmedia-remote.c:195
msgid "Argument to --play must be greater than or equal to 0.\n"
msgstr "L'argument de --play doit être supérieur ou égal à 0.\n"

#: ../src/xfmedia-remote.c:239
msgid "Argument to --playlist-remove must be greater than or equal to 0.\n"
msgstr "L'argument de --playlist-remove doit être supérieur ou égal à 0.\n"

