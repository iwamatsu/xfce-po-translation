#: ../src/main.c:228
#, c-format
msgid ""
"Usage: %s [options] [files]\n"
"\n"
msgstr ""
"Usage : %s [options] [fichiers]\n"
"\n"

#: ../src/main.c:229
#: ../src/xfmedia-remote.c:104
msgid "    -h, --help           Print this help message and exit.\n"
msgstr "    -h, --help           Affiche ce message d'aide et quitte.\n"

#: ../src/main.c:230
#: ../src/xfmedia-remote.c:105
msgid "    -V, --version        Print version information and exit.\n"
msgstr "    -V, --version        Affiche les informations de version et quitte.\n"

#: ../src/main.c:231
msgid "    -v, --verbose        Instruct xine to print useful status information.\n"
msgstr "    -v, --verbose        Indique à xine d'afficher des informations de statut pertinentes.\n"

#: ../src/main.c:232
msgid "    --debug              Instruct xine to print detailed debug information.\n"
msgstr "    --debug              Indique à xine d'afficher des informations détaillées de débugage.\n"

#: ../src/main.c:233
msgid "    -f, --fullscreen     Start xfmedia's video window in fullscreen mode.\n"
msgstr "    -f, --fullscreen     Démarre la fenêtre vidéo de Xfmedia en mode plein écran.\n"

#: ../src/main.c:234
msgid ""
"    -p, --autoplay       Start playing when Xfmedia starts.  (This is the\n"
"                         default if xfmedia is given filenames on the command\n"
"                         line.)\n"
msgstr ""
"    -p, --autoplay       Lance la lecture au démarrage de Xfmedia.  (C'est\n"
"                         le comportement par défaut si un nom de fichier est\n"
"                         passé sur la ligne de commande.)\n"

#: ../src/main.c:237
msgid "    -c, --clear-playlist Start xfmedia with an empty playlist.\n"
msgstr "    -c, --clear-playlist Lance Xfmedia avec une liste de lecture vide.\n"

#: ../src/main.c:238
msgid "    --audio-out=DRIVER   Specify the audio output driver to use.\n"
msgstr "    --audio-out=PILOTE   Spécifie le pilote de sortie audio à utiliser.\n"

#: ../src/main.c:239
msgid "    --video-out=DRIVER   Specify the video output driver to use.\n"
msgstr "    --video-out=PILOTE   Spécifie le pilote de sortie vidéo à utiliser.\n"

#: ../src/main.c:240
msgid ""
"    --vwin-geometry=WxH[+X+Y]\n"
"                         Specify a fixed size and/or position for xfmedia's\n"
"                         video window.\n"
msgstr ""
"    --vwin-geometry=WxH[+X+Y]\n"
"                         Spécifie une taille et/ou position fixe pour la fenêtre                         vidéo de xfmedia.\n"

