#: ../src/main.c:228
#, c-format
msgid ""
"Usage: %s [options] [files]\n"
"\n"
msgstr ""
"Használat: %s [lehetőségek] [fájlok]\n"
"\n"

#: ../src/main.c:229
#: ../src/xfmedia-remote.c:104
msgid "    -h, --help           Print this help message and exit.\n"
msgstr "    -h, --help           Kiírja ezt a súgót és kilép.\n"

#: ../src/main.c:230
#: ../src/xfmedia-remote.c:105
msgid "    -V, --version        Print version information and exit.\n"
msgstr "    -V, --version        Kiírja a verziót és kilép.\n"

#: ../src/main.c:231
msgid "    -v, --verbose        Instruct xine to print useful status information.\n"
msgstr "    -v, --verbose        Utasítja a xine-t a hasznos állapotinfó kiírására.\n"

#: ../src/main.c:232
msgid "    --debug              Instruct xine to print detailed debug information.\n"
msgstr "    --debug              Utasítja a xine-t a részletes hibakereső infó kiírására.\n"

#: ../src/main.c:233
msgid "    -f, --fullscreen     Start xfmedia's video window in fullscreen mode.\n"
msgstr "    -f, --fullscreen     Az Xfmedia filmablakát teljes képernyőben indítja.\n"

#: ../src/main.c:234
msgid ""
"    -p, --autoplay       Start playing when Xfmedia starts.  (This is the\n"
"                         default if xfmedia is given filenames on the command\n"
"                         line.)\n"
msgstr ""
"    -p, --autoplay       Elibdítja a lejátszást az Xfmedia indulásakor.  (Ez az\n"
"                         alap, ha az Xfmedia fájlneveket kap a parancssorban\n"

#: ../src/main.c:237
msgid "    -c, --clear-playlist Start xfmedia with an empty playlist.\n"
msgstr "    -c, --clear-playlist Az Xfmedia programot üres listával indítja.\n"

#: ../src/main.c:238
msgid "    --audio-out=DRIVER   Specify the audio output driver to use.\n"
msgstr "    --audio-out=DRIVER   Megadja a kimeneti hangmeghajtót.\n"

#: ../src/main.c:239
msgid "    --video-out=DRIVER   Specify the video output driver to use.\n"
msgstr "    --video-out=DRIVER   Megadja a kimeneti képmeghajtót.\n"

#: ../src/main.c:240
msgid ""
"    --vwin-geometry=WxH[+X+Y]\n"
"                         Specify a fixed size and/or position for xfmedia's\n"
"                         video window.\n"
msgstr ""
"    --vwin-geometry=WxH[+X+Y]\n"
"                         Megad egy fix méretet és/vagy helyet az Xfmedia\n"
"                         videó ablaknak.\n"

