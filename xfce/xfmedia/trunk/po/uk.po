#: ../src/main.c:227 ../src/xfmedia-remote.c:102
#, c-format
msgid ""
"Xfmedia %s.  Released under the terms of the GNU GPL.  See the COPYING\n"
"file for details.\n"
"\n"
msgstr ""
"Xfmedia %s.  Випущено на умовах GNU GPL.\n"
"Докладнішу інформацію дивіться у файлі COPYING.\n"
"\n"

#: ../src/main.c:228
#, c-format
msgid ""
"Usage: %s [options] [files]\n"
"\n"
msgstr ""
"Використання: %s [параметри] [файли]\n"
"\n"

#: ../src/main.c:229 ../src/xfmedia-remote.c:104
msgid "    -h, --help           Print this help message and exit.\n"
msgstr ""
"    -h, --help           Вивести це повідомлення та завершити роботу.\n"

#: ../src/main.c:230 ../src/xfmedia-remote.c:105
msgid "    -V, --version        Print version information and exit.\n"
msgstr "    -V, --version        Вивести інформацію про версію.\n"

#: ../src/main.c:231
msgid ""
"    -v, --verbose        Instruct xine to print useful status information.\n"
msgstr "    -v, --verbose        вказати xine вивести інформацію про статус.\n"

#: ../src/main.c:232
msgid ""
"    --debug              Instruct xine to print detailed debug information.\n"
msgstr ""
"    --debug              Вказати xine вивести докладну налагоджувальну "
"інформацію.\n"

#: ../src/main.c:233
msgid ""
"    -f, --fullscreen     Start xfmedia's video window in fullscreen mode.\n"
msgstr "    -f, --fullscreen     Відкрити вікно відео xfmedia на весь екран.\n"

#: ../src/main.c:234
msgid ""
"    -p, --autoplay       Start playing when Xfmedia starts.  (This is the\n"
"                         default if xfmedia is given filenames on the "
"command\n"
"                         line.)\n"
msgstr ""
"    -p, --autoplay       Запускати відтворення після запуску Xfmedia.  "
"(Типова поведінка\n"
"                         якщо для xfmedia вказуються назви файлів у "
"командному рядку.)\n"

#: ../src/main.c:237
msgid "    -c, --clear-playlist Start xfmedia with an empty playlist.\n"
msgstr ""
"    -c, --clear-playlist Запускати xfmedia з порожнім списком програвання.\n"

#: ../src/main.c:238
msgid "    --audio-out=DRIVER   Specify the audio output driver to use.\n"
msgstr "    --audio-out=ДРАЙВЕР  Вказати драйвер виводу звуку.\n"

#: ../src/main.c:239
msgid "    --video-out=DRIVER   Specify the video output driver to use.\n"
msgstr "    --video-out=DRIVER   Вказати драйвер виводу відео.\n"

#: ../src/main.c:240
msgid ""
"    --vwin-geometry=WxH[+X+Y]\n"
"                         Specify a fixed size and/or position for xfmedia's\n"
"                         video window.\n"
msgstr ""
"    --vwin-geometry=WxH[+X+Y]\n"
"                         Вказати розмір та/чи позицію вікна відео xfmedia.\n"

#: ../src/main.c:315 ../src/xfmedia-remote.c:167
#, c-format
