#: ../src/main.c:228
#, c-format
msgid ""
"Usage: %s [options] [files]\n"
"\n"
msgstr ""
"Përdorim: %s [mundësi] [kartela]\n"
"\n"

#: ../src/main.c:229
#: ../src/xfmedia-remote.c:104
msgid "    -h, --help           Print this help message and exit.\n"
msgstr "    -h, --help           Shtyp këtë mesazh ndihme dhe dil.\n"

#: ../src/main.c:230
#: ../src/xfmedia-remote.c:105
msgid "    -V, --version        Print version information and exit.\n"
msgstr "    -V, --version        Shtyp të dhëna versioni dhe dil.\n"

#: ../src/main.c:231
msgid "    -v, --verbose        Instruct xine to print useful status information.\n"
msgstr "    -v, --verbose        Udhëzo xine-n të shtypë të dhëna të dobishme gjendjeje.\n"

#: ../src/main.c:232
msgid "    --debug              Instruct xine to print detailed debug information.\n"
msgstr "    --debug              Udhëzo xine-n të shtypë të dhëna të hollësishme debug-u.\n"

#: ../src/main.c:233
msgid "    -f, --fullscreen     Start xfmedia's video window in fullscreen mode.\n"
msgstr "    -f, --fullscreen     Nise të plotë sa ekrani dritaren e videos te xfmedia.\n"

#: ../src/main.c:234
msgid ""
"    -p, --autoplay       Start playing when Xfmedia starts.  (This is the\n"
"                         default if xfmedia is given filenames on the command\n"
"                         line.)\n"
msgstr ""
"    -p, --autoplay       Fillo luajtjen kur niset Xfmedia.  (Kjo është\n"
"                         parazgjedhja nëse xfmedia-s i jepen emra kartelash\n"
"                         në rresht urdhrash.)\n"

#: ../src/main.c:237
msgid "    -c, --clear-playlist Start xfmedia with an empty playlist.\n"
msgstr "    -c, --clear-playlist Nise xfmedia-n me një luajlistë bosh.\n"

#: ../src/main.c:238
msgid "    --audio-out=DRIVER   Specify the audio output driver to use.\n"
msgstr "    --audio-out=PËRUDHËS   Caktoni për përdorim një përudhës përfundimi audio.\n"

#: ../src/main.c:239
msgid "    --video-out=DRIVER   Specify the video output driver to use.\n"
msgstr "    --video-out=PËRUDHËS   Caktoni për përdorim një përudhës përfundimi video.\n"

#: ../src/main.c:240
msgid ""
"    --vwin-geometry=WxH[+X+Y]\n"
"                         Specify a fixed size and/or position for xfmedia's\n"
"                         video window.\n"
msgstr ""
"    --vwin-geometry=WxH[+X+Y]\n"
"                         Caktoni një madhësi të fiksuar dhe/ose vend për dritaren\n"
"                         e videos në xfmedia.\n"

