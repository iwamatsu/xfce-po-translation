#: ../src/xfmedia-remote.c:103
#, c-format
msgid ""
"Usage: %s [options]\n"
"\n"
msgstr "Forma d'ús: %s [OPCIONS]\n"

#: ../src/xfmedia-remote.c:106
msgid ""
"    --session=N          Specify the number of the already-running instance "
"of\n"
"                         Xfmedia.  (Defaults to 0, or the first instance "
"found.)\n"
msgstr ""
"    --session=N          Especifiqueu el número d'instància ja executant-se\n"
"                         d'xfmedia. (Per defecte 0 o la primera trobada).\n"

#: ../src/xfmedia-remote.c:108
msgid ""
"    --ping               Checks to see if an instance of Xfmedia is "
"running.\n"
msgstr ""
"    --ping               Comprova si una instància d'xfmedia s'està "
"executant.\n"

#: ../src/xfmedia-remote.c:109
msgid ""
"    -p, --play[=N]       Causes Xfmedia to play the file at playlist idx N.\n"
"                         If N is not specified, Xfmedia will play the "
"currently-\n"
"                         selected entry, or, if no item is selected, the "
"first\n"
"                         entry.  If used in conjunction with --playlist-"
"add,\n"
"                         will cause Xfmedia to play the newly-added entry.\n"
msgstr ""
"    -p, --play[=N]       Fa que xfmedia reprodueixi l'element N de la llista "
"de\n"
"                         reproducció. Si no s'especifica N, xfmedia "
"reproduirà\n"
"                         l'entrada seleccionada o, si no hi ha cap "
"seleccionat,\n"
"                         el primer de la llista. Si es fa servir junt amb\n"
"                         «--playlist-add» s'executarà l'entrada afegida.\n"

#: ../src/xfmedia-remote.c:114
msgid ""
"    --toggle             Causes Xfmedia to pause if playing, or play if "
"paused\n"
"                         or stopped.\n"
msgstr ""
"    --toggle             Si xfmedia està reproduint li farà fer una pausa, "
"si\n"
"                         està aturat o en pausa li farà reproduir.\n"

#: ../src/xfmedia-remote.c:116
msgid "    --pause              Causes Xfmedia to pause/unpause.\n"
msgstr "    --pause              Fa que l'xfmedia entri o acabi la pausa.\n"

#: ../src/xfmedia-remote.c:117
msgid "    --stop               Causes Xfmedia to stop playback.\n"
msgstr "    --stop               Fa que l'xfmedia aturi la reproducció.\n"

#: ../src/xfmedia-remote.c:118
msgid ""
"    --previous           Causes Xfmedia to play the entry played before the\n"
"                         currently-selected (or -playing) entry.\n"
msgstr ""
"    --previous           Fa que l'xfmedia reprodueixi l'entrada anterior a "
"la\n"
"                         seleccionada (o la «-playing»).\n"

#: ../src/xfmedia-remote.c:120
msgid ""
"    --next               Causes Xfmedia to play the next entry in the "
"playlist.\n"
"                         If Xfmedia is in shuffle mode, a new entry will be\n"
"                         selected randomly.\n"
msgstr ""
"    --next               Fa que l'xfmedia reprodueixi la següent entrada de "
"la\n"
"                         llista de reproducció. Si està en mode aleatori, "
"es\n"
"                         seleccionarà una entrada al atzar.\n"

#: ../src/xfmedia-remote.c:123
msgid ""
"    -a, --playlist-add=FILE\n"
"                         Adds FILE to the end of the playlist.\n"
msgstr ""
"    -a, --playlist-add=FITXER\n"
"                         Afegeix el FITXER al final de la llista de "
"reproducció.\n"

#: ../src/xfmedia-remote.c:125
msgid ""
"    -r, --playlist-remove=N\n"
"                         Removes the specified playlist entry from the "
"playlist.\n"
msgstr ""
"    -r, --playlist-remove=N\n"
"                         Treu l'entrada N de la llista de reproducció.\n"

#: ../src/xfmedia-remote.c:127
msgid ""
"    -c, --playlist-clear\n"
"                         Removes all entries from the playlist.\n"
msgstr ""
"    -c, --playlist-clear\n"
"                         Treu totes les entrades de la llista de "
"reproducció.\n"

#: ../src/xfmedia-remote.c:129
msgid ""
"    -l, --playlist-load=FILE\n"
"                         Loads the specified playlist.  Note that this does "
"NOT\n"
"                         first clear the playlist.\n"
msgstr ""
"    -l, --playlist-load=FITXER\n"
"                         Carrega la llista de reproducció. Noteu que això "
"no\n"
"                         neteja abans la llista.\n"

#: ../src/xfmedia-remote.c:132
msgid ""
"    -s, --playlist-save=FILE\n"
"                         Saves the current playlist to the specified FILE.\n"
msgstr ""
"    -s, --playlist-save=FITXER\n"
"                         Desa la llista de reproducció en el FITXER "
"especificat.\n"

#: ../src/xfmedia-remote.c:134
msgid "    -Q, --quit           Instructs Xfmedia to quit.\n"
msgstr "    -Q, --quit           Per fer acabar a l'xfmedia.\n"

#: ../src/xfmedia-remote.c:135
msgid ""
"    -j, --jump-to-file   Causes Xfmedia to show the main window (if not "
"visible)\n"
"                         and show the jump-to-file entry box.\n"
msgstr ""
"    -j, --jump-to-file   Fa que l'xfmedia mostri la seva pantalla (si no és\n"
"                         visible) i mostri el plafó per saltar a un fitxer.\n"

#: ../src/xfmedia-remote.c:180
msgid "Argument to --session must be greater than or equal to 0.\n"
msgstr "L'argument de «--session» ha de ser 0  més gran.\n"

#: ../src/xfmedia-remote.c:195
msgid "Argument to --play must be greater than or equal to 0.\n"
msgstr "L'argument de «--play» ha de ser 0 o més gran.\n"

#: ../src/xfmedia-remote.c:239
msgid "Argument to --playlist-remove must be greater than or equal to 0.\n"
msgstr "L'argument de «--playlist-remove» ha de ser 0 o més gran.\n"

#: ../src/xfmedia-remote.c:276
#, c-format
