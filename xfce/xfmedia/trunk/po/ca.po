#: ../src/main.c:228
#, c-format
msgid ""
"Usage: %s [options] [files]\n"
"\n"
msgstr ""
"Forma d'ús: %s [opcions] [fitxers]\n"
"\n"

#: ../src/main.c:229 ../src/xfmedia-remote.c:104
msgid "    -h, --help           Print this help message and exit.\n"
msgstr "    -h, --help           Mostra aquest missatge d'ajuda i surt.\n"

#: ../src/main.c:230 ../src/xfmedia-remote.c:105
msgid "    -V, --version        Print version information and exit.\n"
msgstr "    -V, --version        Mostra informació de la versió i surt.\n"

#: ../src/main.c:231
msgid ""
"    -v, --verbose        Instruct xine to print useful status information.\n"
msgstr ""
"    -v, --verbose        Fes que el xine mostri informació útil de l'estat.\n"

#: ../src/main.c:232
msgid ""
"    --debug              Instruct xine to print detailed debug information.\n"
msgstr ""
"    --debug              Fes que el xine mostri informació de depuració\n"
"                         detallada.\n"

#: ../src/main.c:233
msgid ""
"    -f, --fullscreen     Start xfmedia's video window in fullscreen mode.\n"
msgstr ""
"    -f, --fullscreen     Inicia la finestra de vídeo d'xfmedia a pantalla\n"
"                         sencera.\n"

#: ../src/main.c:234
msgid ""
"    -p, --autoplay       Start playing when Xfmedia starts.  (This is the\n"
"                         default if xfmedia is given filenames on the "
"command\n"
"                         line.)\n"
msgstr ""
"    -p, --autoplay       Comença a reproduir al iniciar xfmedia. (Això és "
"el\n"
"                         comportament per defecte d'xfmedia quan hi han "
"noms\n"
"                         de fitxer a la línia d'ordres).\n"

#: ../src/main.c:237
msgid "    -c, --clear-playlist Start xfmedia with an empty playlist.\n"
msgstr ""
"    -c, --clear-playlist Inicia xfmedia amb una llista de reproducció "
"buida.\n"

#: ../src/main.c:238
msgid "    --audio-out=DRIVER   Specify the audio output driver to use.\n"
msgstr ""
"    --audio-out=DISPOSITIU\n"
"                        Especifiqueu el dispositiu de sortida d'àudio.\n"

#: ../src/main.c:239
msgid "    --video-out=DRIVER   Specify the video output driver to use.\n"
msgstr ""
"    --video-out=DISPOSITIU\n"
"                        Especifiqueu el dispositiu de sortida de vídeo.\n"

#: ../src/main.c:240
msgid ""
"    --vwin-geometry=WxH[+X+Y]\n"
"                         Specify a fixed size and/or position for xfmedia's\n"
"                         video window.\n"
msgstr ""
"    --vwin-geometry=WxH[+X+Y]\n"
"                         Especifiqueu una mida fixe i/o una posició per la\n"
"                         finestra de vídeo d'xfmedia.\n"

