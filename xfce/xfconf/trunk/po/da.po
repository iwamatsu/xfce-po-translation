#: xfsettingsd/main.c:52
#: xfconf-query/main.c:139
msgid "Version information"
msgstr "Versioninformation"

#: xfsettingsd/main.c:56
#: xfconf-query/main.c:159
msgid "Verbose output"
msgstr "Uddybende uddata"

#: xfsettingsd/main.c:60
msgid "Force replace existing any xsettings daemon"
msgstr "Tving udskiftning af eksisterende dæmon-xindstillinger"

#: xfsettingsd/main.c:64
msgid "Start in debug mode (don't fork to the background)"
msgstr "Start i fejlsøgningstilstand (forgren ikke til baggrunden)"

#: xfsettingsd/main.c:131
#, c-format
msgid ""
"%s: %s\n"
"Try %s --help to see a full list of available command line options.\n"
msgstr ""
"%s: %s\n"
"Prøv %s --help til fuld liste over tilgængelige kommandolinjer.\n"

#: xfconfd/xfconf-backend-perchannel-xml.c:266
#, c-format
msgid "Unable to create configuration directory"
msgstr "Kunne ikke oprette indstillingsmappe"

#: xfconfd/xfconf-backend-perchannel-xml.c:313
#, c-format
msgid "You don't have permission to modify property \"%s\" on channel \"%s\""
msgstr "Du har ikke rettigheder til at ændre egenskaber \"%s\" for kanal \"%s\""

#: xfconfd/xfconf-backend-perchannel-xml.c:364
#: xfconfd/xfconf-backend-perchannel-xml.c:439
#: xfconfd/xfconf-backend-perchannel-xml.c:581
#: xfconfd/xfconf-backend-perchannel-xml.c:602
#, c-format
msgid "Property \"%s\" does not exist on channel \"%s\""
msgstr "Egenskab \"%s\" findes ikke på kanal \"%s\""

#: xfconfd/xfconf-backend-perchannel-xml.c:548
#, c-format
msgid "Unable to remove channel \"%s\": %s"
msgstr "Kunne ikke fjerne kanal \"%s\".%s"

#: xfconfd/xfconf-backend-perchannel-xml.c:1105
#: xfconfd/xfconf-backend-perchannel-xml.c:1175
#, c-format
msgid "Invalid type for <%s>: \"%s\""
msgstr "Ugyldig type for <%s>: \"%s\""

#: xfconfd/xfconf-backend-perchannel-xml.c:1117
#: xfconfd/xfconf-backend-perchannel-xml.c:1187
#, c-format
msgid "Unable to parse value of type \"%s\" from \"%s\""
msgstr "Kunne ikke fortolke værdi af type \"%s\" fra \"%s\""

#: xfconfd/xfconf-backend-perchannel-xml.c:1166
#, c-format
msgid "The type attribute of <value> cannot be an array"
msgstr "Typeattributten på <value> kan ikke være en tabel"

#: xfconfd/xfconf-backend-perchannel-xml.c:1434
#: xfconfd/xfconf-backend-perchannel-xml.c:1677
#, c-format
msgid "Channel \"%s\" does not exist"
msgstr "Kanal \"%s\" findes ikke"

#: xfconfd/xfconf-backend-perchannel-xml.c:1722
#, c-format
msgid "Unable to write channel \"%s\": %s"
msgstr "Kunne ikke skrive til kanal \"%s\": %s"

#: xfconfd/xfconf-backend-factory.c:70
#, c-format
msgid "Unable to find Xfconf backend of type \"%s\""
msgstr "Kunne ikke finde Xfconf bagende af type \"%s\""

#: xfconfd/xfconf-backend.c:103
#, c-format
msgid "Property names must start with a '/' character"
msgstr "Egenskabnavne skal starte med '/' karakter"

#: xfconfd/xfconf-backend.c:112
#, c-format
msgid "The root element ('/') is not a valid property name"
msgstr "Rodelementet ('/') er ikke et gyldigt egenskabnavn"

#: xfconfd/xfconf-backend.c:126
#, c-format
msgid "Property names can only include the ASCII characters A-Z, a-z, 0-9, '_', '-', '<' and '>', as well as '/' as a separator"
msgstr "Egenskabnavne kan kun indeholde ASCII-karakterne A-Z, a-z, 0-9, '_', '-', '<' og '>', samt  '/'  som adskiller"

#: xfconfd/xfconf-backend.c:135
#, c-format
msgid "Property names cannot have two or more '/' characters in a row"
msgstr "Egenskabnavne kan ikke indeholde mere end to '/' karakterer efter hinanden"

#: xfconfd/xfconf-backend.c:146
#, c-format
msgid "Property names cannot end with a '/' character"
msgstr "Egenskabnavne kan ikke ende på '/' karakter"

#: xfconfd/xfconf-backend.c:163
#, c-format
msgid "Channel name cannot be an empty string"
msgstr "Kanalnavn kan ikke være tom streng"

#: xfconfd/xfconf-backend.c:176
#, c-format
msgid "Channel names can only include the ASCII characters A-Z, a-z, 0-9, '_', and '-'"
msgstr "Kanalnavne kan kun indeholde ASCII karaktererne A-Z, a-z, 0-9, '_', og '-'"

#: xfconfd/xfconf-backend.c:394
#, c-format
msgid "The property name can only be empty or \"/\" if a recursive remove was specified."
msgstr "Egenskabnavne kan kun være tomme eller \"/\" hvis rekursiv fjernelse blev angivet."

#: xfconfd/main.c:127
msgid "Prints the xfconfd version"
msgstr "Udskriver xfconfd-version"

#: xfconfd/main.c:129
msgid "Configuration backends to use.  The first backend specified is opened read/write; the others, read-only."
msgstr "Indstillingsbagende til anvendelse.  Den første bagende der angives åbnes med læs/skriv; de andre som skrivebeskyttet."

#: xfconfd/main.c:158
msgid "Xfce configuration daemon"
msgstr "Xfce-indstillingsdæmon"

#: xfconfd/main.c:160
msgid "Report bugs to http://bugs.xfce.org/\n"
msgstr "Rapporter fejl til  http://bugs.xfce.org/\n"

#: xfconfd/xfconf-daemon.c:448
#, c-format
msgid "Another Xfconf daemon is already running"
msgstr "En anden Xfconf-dæmon kører allerede"

#: xfconfd/xfconf-daemon.c:484
#, c-format
msgid "No backends could be started"
msgstr "Ingen bagende kunne startes"

#: xfconf-query/main.c:143
msgid "pick the channel"
msgstr "vælg kanal"

#: xfconf-query/main.c:147
msgid "pick the property"
msgstr "vælg egenskab"

#: xfconf-query/main.c:151
msgid "set (change the value)"
msgstr "indstil (ændre værdi)"

#: xfconf-query/main.c:155
msgid "List properties"
msgstr "Liste egenskaber"

#: xfconf-query/main.c:163
msgid "Create new entry"
msgstr "Opret ny post"

#: xfconf-query/main.c:167
msgid "Specify the property value type"
msgstr "Angiv egenskabværditype"

#: xfconf-query/main.c:171
msgid "Remove property"
msgstr "Fjern egenskab"

#: xfconf-query/main.c:175
msgid "Export channel"
msgstr "Eksporter kanal"

#: xfconf-query/main.c:179
msgid "Import channel"
msgstr "Importer kanal"

#: xfconf-query/main.c:276
#, c-format
msgid "Property \"%s\" doesn't exist on channel \"%s\".\n"
msgstr "Egenskab \"%s\" findes ikke på kanal \"%s\".\n"

#: xfconf-query/main.c:284
#: xfconf-query/main.c:302
msgid "(unknown)"
msgstr "(ukendt)"

#: xfconf-query/main.c:293
#, c-format
msgid ""
"Value is an array with %d items:\n"
"\n"
msgstr ""
"Værdi er en tabel med %d elementer:\n"
"\n"

#: xfconf-query/main.c:316
#, c-format
msgid ""
"Property \"%s\" does not exist on channel \"%s\".  If you would\n"
"like to create a new property, use the --create option.\n"
msgstr ""
"Egenskab \"%s\" findes ikke på denne kanal \"%s\".  Hvis du ønsker\n"
"at oprette ny egenskab brug --create indstilling.\n"

#: xfconf-query/main.c:324
#, c-format
msgid "When creating a new property, you must specify the value type.\n"
msgstr "Når der oprettes en ny egenskab, skal du angive værditypen.\n"

#: xfconf-query/main.c:334
#, c-format
msgid "Failed to get existing type for value.\n"
msgstr "Kunne ikke få eksisterende type for værdi.\n"

#: xfconf-query/main.c:352
#, c-format
msgid "Unable to determine type of value.\n"
msgstr "Kunne ikke afgøre type af værdi.\n"

#: xfconf-query/main.c:358
#, c-format
msgid "You must specify a value type to change an array into a single value.\n"
msgstr "Du skal angive en værditype for at ændre en tabel til en enkeltværdi.\n"

#: xfconf-query/main.c:368
#: xfconf-query/main.c:430
#, c-format
msgid "Unable to convert \"%s\" to type \"%s\"\n"
msgstr "Kunne ikke konvertere \"%s\" til type \"%s\"\n"

#: xfconf-query/main.c:375
#: xfconf-query/main.c:445
#, c-format
msgid "Failed to set property.\n"
msgstr "Kunne ikke sætte egenskab.\n"

#: xfconf-query/main.c:400
#, c-format
msgid "Have %d new values, but could only determine %d types.\n"
msgstr "Har %d nye værdier, men kunne kun afgøre %d typer.\n"

#: xfconf-query/main.c:421
#, c-format
msgid "Unable to determine type of value at index %d.\n"
msgstr "Kunne ikke afgøre type af værdi ved indeks %d.\n"

#: xfconf-query/main.c:463
#, c-format
msgid "Channel '%s' contains no properties\n"
msgstr "Kanal '%s' indeholder ingen egenskaber\n"

